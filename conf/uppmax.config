/*
vim: syntax=groovy
-*- mode: groovy;-*-
 * -------------------------------------------------
 *  Nextflow config file for UPPMAX (milou / irma)
 * -------------------------------------------------
 * Defines reference genomes, using iGenome paths
 * Imported under the default 'standard' Nextflow
 * profile in nextflow.config
 */


process {
  executor = 'slurm'
  cpus = 1
  memory = '16 GB'
  time = '48h'
  clusterOptions = {
    "-A $params.project"
  }
  
  errorStrategy = task.exitStatus == 143 ? 'retry' : 'finish'
  maxRetries = 3
  maxErrors = '-1'
  
  // Process specific requirements
  $makeSTARindex {
    module = ['bioinfo-tools', 'star']
    cpus 12
    memory '30GB'
    time  { 5.h * task.attempt }
    errorStrategy = task.exitStatus == 143 ? 'retry' : 'terminate'
  }
  $makeBED12 {
    module = ['bioinfo-tools', 'BEDOPS']
    time  { 2.h * task.attempt }
    errorStrategy = task.exitStatus == 143 ? 'retry' : 'terminate'
  }
  $fastqc {
    module = ['bioinfo-tools', 'FastQC']
    memory { 2.GB * task.attempt }
    time { 4.h * task.attempt }
    errorStrategy = task.exitStatus == 143 ? 'retry' : 'ignore'
  }
  $trim_galore {
    module = ['bioinfo-tools', 'TrimGalore']
    cpus 2
    memory { 4.GB * task.attempt }
    time { 8.h * task.attempt }
    errorStrategy = task.exitStatus == 143 ? 'retry' : 'terminate'
  }
  $star {
    module = ['bioinfo-tools', 'star']
    cpus 10
    memory '80GB'
    time  { 5.h * task.attempt }
    errorStrategy = task.exitStatus == 143 ? 'retry' : 'terminate'
  }
  $rseqc {
    module = ['bioinfo-tools', 'rseqc', 'samtools']
    memory { 32.GB * task.attempt }
    time  { 7.h * task.attempt }
  }
  $preseq {
    module = ['bioinfo-tools', 'preseq']
    memory { 4.GB * task.attempt }
    time { 2.h * task.attempt }
  }
  $markDuplicates {
    module = ['bioinfo-tools', 'picard/2.0.1']
    memory { 16.GB * task.attempt }
    time { 2.h * task.attempt }
  }
  $dupradar {
    module = ['bioinfo-tools', 'R/3.2.3']
    memory { 16.GB * task.attempt }
    time { 2.h * task.attempt }
  }
  $featureCounts {
    module = ['bioinfo-tools', 'subread']
    memory { 4.GB * task.attempt }
    time { 2.h * task.attempt }
  }
  $stringtieFPKM {
    module = ['bioinfo-tools', 'StringTie']
    memory { 4.GB * task.attempt }
    time { 2.h * task.attempt }
  }
  $sample_correlation {
    module = ['bioinfo-tools', 'R/3.2.3']
    memory { 16.GB * task.attempt }
    time { 2.h * task.attempt }
  }
  $multiqc {
    // NB: Set $multiqc.module = false if you have your own installation
    // of MultiQC outside of the environment module system.
    module = ['bioinfo-tools', 'MultiQC']
    memory '4GB'
    time '4h'
    errorStrategy = 'ignore'
  }
}

