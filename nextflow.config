
profiles {
  
  module {
    process.$fastqc.module = ['bioinfo-tools', 'FastQC']
    process.$trim_galore.module = ['bioinfo-tools', 'TrimGalore']
    process.$star.module = [ 'bioinfo-tools', 'star' ]
    process.$rseqc.module = [ 'bioinfo-tools', 'rseqc', 'samtools' ]
    process.$preseq.module = [ 'bioinfo-tools', 'preseq' ]
    process.$markDuplicates.module = [ 'bioinfo-tools', 'picard/2.0.1']
    process.$dupradar.module = [ 'bioinfo-tools', 'R/3.2.3' ]
    process.$featureCounts.module = [ 'bioinfo-tools', 'subread' ]
    process.$stringtieFPKM.module = [ 'bioinfo-tools', 'StringTie' ]
    process.$sample_correlation.module = [ 'bioinfo-tools', 'R/3.2.3' ]
    process.$multiqc.module = [ 'bioinfo-tools' ]
  }
  
  docker {
    process.container = 'your/image'
    docker.enabled = true
  }

}